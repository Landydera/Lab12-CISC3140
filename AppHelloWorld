import React from 'react';

class App extends React.Component {
  render() {
    return (
      <h1>Hello, {this.props.name}</h1>
    );
  }
}

function App() {
  return (
    <div>
      <Hello name="Ann" />
      <Hello name="Peter" />
    </div>
  );
}

ReactDOM.render(
  <App />,
  document.getElementById('root')
);
